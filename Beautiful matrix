//problem: https://codeforces.com/contest/263/problem/A

#include <bits/stdc++.h>
using  namespace std;
int main(){
    int k;
    int m[5][5];
    for (int i=0;i<=4;i++){
        for (int j=0;j<=4;j++){
            cin>>m[i][j];
        if (m[i][j]==1){
            k=abs(i-2)+abs(j-2);
        }
 
        }
    
    }
    cout<<k<<endl;
   
    
    
    return 0;
}
