//problem: https://codeforces.com/contest/1996/problem/A

#include <bits/stdc++.h>
using  namespace std;
int main(){
    int t,n,x;
    cin>>t;
    while (t--){
        cin >>n;
        if (n%4==0){
            x=(n/4);
        }else{
            x=(n/4)+1;
        }
        cout<<x<<endl;
    }
    return 0;
        }
