//problem: https://codeforces.com/contest/1703/problem/B

#include <bits/stdc++.h>
using  namespace std;
int main(){
    int n,t,b,a,z,k;
    cin>>t;
    string s;
    while(t--){
        string s;
        
        cin>>n;
        cin>>s;
        set<char>x;
        for (int i=0;i<n;i++){
            x.insert(s[i]);
 
        }
        k=x.size();
        a=k*2;
        b=s.length();
        z=b-k;
        a+=z;
        cout<<a<<endl;
 
            }
    return 0;
        }
         
