//problem: https://codeforces.com/contest/1896/problem/A

#include <bits/stdc++.h>
using  namespace std;
int main(){
    int t;
    cin>>t;
    
    while (t--){
        int n,i;
        cin >>n;
        vector <int> arr;
        
        for( i=0;i<n;i++){
            int x;
            cin>>x;
            arr.push_back(x);
        }
        if (arr[0]==1){
            cout<<"Yes"<<endl;
        }else{
            cout<<"No"<<endl;
        }
        
        
    }return 0;
        }
